{
  "name": "manger",
  "version": "0.0.1",
  "description": "cache RSS and Atom feeds",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "show-conf": "node config.js",
    "repl": "node repl.js",
    "test": "export NODE_TEST=1 ; tap test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/manger.git"
