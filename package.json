    "atom",
    "xml",
    "cache",
    "json",
    "feed",
    "normalize"
  ],
  "dependencies": {
    "pickup": "0.1.x",
    "levelup": "0.9.x",
    "leveldown": "0.5.x"
  },
  "devDependencies": {
    "rimraf": "2.2.x",
    "tap": "0.4.x"
  },
  "engines": {
    "node": "0.10.x"
  },
